// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URI")
}

model BlogPost {
  id           Int    @id @default(autoincrement())
  title        String
  date         String
  description  String
  thumbnailUrl String
  demoUrl      String
}
